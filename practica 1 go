package main

import "fmt"

func main() {

	//declaracion de variables
	var name string = "geisy"
	var edad = 18
	var meta string = "Deseo trabajar en la nasa"
	const pais string = "rep dom"
	
	//impresion de mensaje
	fmt.Println("Hola, me llamo", name, "tengo", edad, "anos, y mi meta profesional es la siguiente:", meta, "y por otro lado vivo en:", pais)
}
